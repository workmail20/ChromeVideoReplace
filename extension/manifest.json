{
    "manifest_version": 2,
    "name": "Video 124",
    "version": "1.0",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "declarativeNetRequest",
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "images/i16.png",
        "32": "images/i32.png",
        "48": "images/i48.png",
        "128": "images/i128.png"
    },
    "action": {
        "default_icon": {
            "16": "images/i16.png",
            "32": "images/i32.png",
            "48": "images/i48.png",
            "128": "images/i128.png"
        }
    }
}
